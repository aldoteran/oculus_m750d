<launch>
<!--Launch file to run the experiments. Remeber to run the RQT dynamic
    reconfig on the remote computer. Also remember to calibrate camera
    if the resolution is different-->

	<!--Oculus main node-->
	<!--<node pkg="oculus_sonar" type="oculus_sonar_node.py" name="sonar_imager" output="screen"/>-->

	<!--Run SBG ellipse AHRS-->
	<include file="$(find fs-slam)/launch/sbg_device.launch" />

	<!--Logitech Camera run-->
	<node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
	<param name="video_device" value="/dev/video1" />
	<param name="image_width" value="640" />
	<param name="image_height" value="480" />
	<param name="pixel_format" value="yuyv" />
	<param name="camera_frame_id" value="usb_cam" />
	<param name="io_method" value="mmap"/>
	</node>
</launch>
